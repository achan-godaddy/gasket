{
  "name": "@gasket/plugin-jest",
  "version": "6.17.1",
  "description": "Integrated jest into your application.",
  "main": "lib",
  "files": [
    "generator",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "jest",
    "gasket",
    "plugin"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Salil Agrawal <sagrawal@godaddy.com>",
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-jest",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.7.0",
    "eslint-config-godaddy-react": "^8.0.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "jest": "^27.4.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "eslintConfig": {
    "extends": [
      "godaddy-react",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  }
}
