{
  "name": "@gasket/plugin-warehouse",
  "version": "1.0.0",
  "description": "Gasket Warehouse Plugin",
  "main": "lib",
  "bin": {
    "wrhs-publish": "./bin/wrhs-publish"
  },
  "files": [
    "!lib/**/*.test.js",
    "lib",
    "bin"
  ],
  "scripts": {
    "lint": "eslint . --ignore-pattern \"generator/jest/*\" --ignore-pattern \"generator/mocha/*\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/godaddy/gasket.git"
  },
  "keywords": [
    "gasket",
    "plugin",
    "warehouse"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/gasket/issues"
  },
  "homepage": "https://github.com/godaddy/gasket/tree/main/packages/gasket-plugin-warehouse",
  "dependencies": {
    "out-of-band-cache": "^1.2.3",
    "wrhs": "^1.1.1"
  },
  "devDependencies": {
    "eslint": "^8.16.0",
    "eslint-config-godaddy-react": "^8.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "jest": "^28.1.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy-react"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    },
    "overrides": [
      {
        "files": [
          "**/*.test.js",
          "**/*.test.jsx"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "eslintIgnore": [
    "coverage"
  ]
}
